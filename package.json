{
  "name": "html-to-markdown",
  "version": "1.0.0",
  "description": "A system for converting HTML files to markdown.",
  "main": "index.js",
  "scripts": {
    "fm": "node front-matter.js news/*/**.html",
    "md": "node markdown.js news/*/**.html",
    "cat": "node concat.js news/*/**.html",
    "clean": "node clean.js news/*/**.html",
    "build": "npm run fm && npm run md && npm run cat && npm run clean"
  },
  "keywords": [
    "markdown",
    "html",
    "utility"
  ],
  "author": "Christopher Murphy",
  "license": "ISC",
  "devDependencies": {
    "concat": "^1.0.3",
    "jsdom": "^11.0.0",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "to-markdown": "^3.0.4"
  },
  "dependencies": {}
}
